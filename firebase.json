{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/recommend",
        "run": {
          "serviceId": "profitpoke-api",
          "region": "us-central1"
        }
      },
      {
        "source": "/callback",
        "run": {
          "serviceId": "profitpoke-api",
          "region": "us-central1"
        }
      },
      {
        "source": "/postback",
        "run": {
          "serviceId": "profitpoke-api",
          "region": "us-central1"
        }
      },
      {
        "source": "/**",
        "run": {
          "serviceId": "profitpoke-api",
          "region": "us-central1"
        }
      }
    ]
  }
}
